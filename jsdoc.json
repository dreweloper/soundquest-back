{
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",   
        "include": ["./controllers", "./helpers", "./models", "./routers", "./app.js"],                         
        "exclude": ["node_modules/|docs"]              
    },
    "plugins": [],
    "recurseDepth": 10,                         
    "opts": {
        "destination": "./docs/",          
        "recurse": true   
    }                       
}